<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anonymous.ucss">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/univicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splashscreen.SplashScreen"
            android:label="@string/app_name" />
        <activity
            android:name=".registration.Register"
            android:label="@string/user_registration" />
        <activity
            android:name=".student.Student"
            android:label="@string/title_activity_student"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".login.Login"
            android:label="Login"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".lecturer.Lecturer"
            android:label="@string/title_activity_lecturer"
            android:theme="@style/AppTheme" />
        <activity android:name=".student.stu_sms" />
        <activity android:name=".MapsActivity" />
        <activity android:name=".lecturer.lect_send" />
        <activity android:name=".lecturer.Onlyone" />
        <activity android:name=".lecturer.upload" />
        <activity android:name=".view_students" />
        <activity android:name=".lecturer.lect_sms" />
        <activity android:name=".admin.admin" />
        <activity android:name=".student.view_details" />
        <activity android:name=".view_individual" />
        <activity android:name=".student.stu_mail" />
        <activity android:name=".user_delete" />
        <activity android:name=".edit_user" />
        <activity android:name=".lecturer.upload_home" />
        <activity android:name=".student.download_files"></activity>
    </application>

</manifest>